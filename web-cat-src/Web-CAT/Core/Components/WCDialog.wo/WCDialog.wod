Cancel : WCButton {
    label = cancelLabel;
    class = "~hasOkButton ? 'neg' : null";
}

Dialog : WCContentPane {
    dojoType = "webcat.Dialog";
    alwaysDynamic = true;
    showsLoadingMessageOnRefresh = true;
    title = title;
    id = id;
}

Form : WCForm {
}

OK : WCButton {
    id = idFor.okButtonId;
    label = okLabel;
    class = "pos";
    action = okPressed;
    remote = true;
}
