<project default="jar_export" name="build">
    <target name="jar_export">
        <jar destfile="../Reporter/Libraries/webcat-oda-commons.jar"
            basedir="bin"
            includes="**/*.class"/>
        <eclipse.refreshLocal
            resource="Reporter/Libraries"
            depth="infinite"/>
    </target>
</project>
