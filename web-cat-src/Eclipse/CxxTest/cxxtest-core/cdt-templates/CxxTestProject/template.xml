<?xml version="1.0" encoding="ISO-8859-1"?>
<template type="ProjTempl"
	version="1.0"
	supplier="Virginia Tech Computer Science Department"
	revision="1.0"
	author="Tony Allevato"
	id="CxxTestProject"
	label="%CxxTestProject.label"
	description="%CxxTestProject.description"
	help="help.html">
	
	<process type="org.eclipse.cdt.managedbuilder.core.NewManagedProject">
		<simple name="name" value="$(projectName)" /> 
		<simple name="artifactExtension" value="exe" /> 
		<simple name="isCProject" value="false" /> 
	</process>

	<process type="net.sf.webcat.eclipse.cxxtest.AddCxxTestNature">
		<simple name="projectName" value="$(projectName)"/>
	</process>

</template>
